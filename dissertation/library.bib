@article{Vara2018,
abstract = {Abstract Smoke particles ejected into the atmosphere from biomass burning can modify the atmospheric composition around and even far from the sources. In late winter and early spring, biomass burning emissions from inland regions can be efficiently transported to urban areas in southâeastern South America, thus affecting air quality in those areas. In this study, the Weather Research and Forecasting with Chemistry (WRFâChem) model was applied in order to investigate the impact of biomass burning sources on aerosol loadings and properties over the S{\~{a}}o Paulo Metropolitan Area (SPMA), in southâeastern Brazil, during the period from 19 August to 3 September 2014. The model performance was evaluated using available aerosol measurements from the Narrowing the Uncertainties on Aerosol and Climate Change in S{\~{a}}o Paulo State (NUANCEâSPS) project. The combined application of aerosol data and WRFâChem simulations made it possible to represent some of the most important aerosol properties, such as particle number concentration (PNC) and cloud condensation nuclei (CCN) activation, in addition to evaluation of the impact of biomass burning by analysing a fiveâday transport event, from 22 August to 26 August 2014. During this transport event, differences in the average predicted PM2.5 concentration reached 15 $\mu$g mâ3 (peaking at 20 $\mu$g mâ3 during the nightâtime hours) over the SPMA, compared with 35 $\mu$g mâ3 over inland areas northwest and north of the SPMA. Biomass burning accounted for up to 20 {\%} of the baseline PNCâ and CCNâweighted relative differences over the SPMA (2300 cmâ3 and 1400 cmâ3, respectively).},
author = {Vara-Vela, Angel and {de F{\'{a}}tima Andrade}, Maria and Zhang, Yang and Kumar, Prashant and Ynoue, Rita Yuri and Souto-Oliveira, Carlos Eduardo and {da Silva Lopes}, F{\'{a}}bio Juliano and Landulfo, Eduardo},
doi = {10.1029/2018JD028768},
file = {:Users/adelgado/Library/Application Support/Mendeley Desktop/Downloaded/Vara-Vela et al. - 2018 - Modeling of Atmospheric Aerosol Properties in the S{\~{a}}o Paulo Metropolitan Area Impact of Biomass Burning.pdf:pdf},
issn = {2169897X},
journal = {Journal of Geophysical Research: Atmospheres},
keywords = {air quality modeling,pesquisa},
mendeley-tags = {air quality modeling,pesquisa},
month = {sep},
number = {17},
pages = {9935--9956},
title = {{Modeling of Atmospheric Aerosol Properties in the S{\~{a}}o Paulo Metropolitan Area: Impact of Biomass Burning}},
url = {http://doi.wiley.com/10.1029/2018JD028768},
volume = {123},
year = {2018}
}
