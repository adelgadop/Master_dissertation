journal = {Climatic Change},
keywords = {climate change,pesquisa},
mendeley-tags = {climate change,pesquisa},
month = {nov},
number = {1-2},
pages = {77--94},
title = {{RCP4.5: a pathway for stabilization of radiative forcing by 2100}},
url = {http://link.springer.com/10.1007/s10584-011-0151-4},
volume = {109},
year = {2011}
}

