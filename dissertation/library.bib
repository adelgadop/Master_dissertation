mendeley-tags = {air quality,pesquisa},
month = {may},
number = {10},
pages = {e2019GL086901},
publisher = {American Geophysical Union (AGU)},
title = {{On the Changing Role of the Stratosphere on the Tropospheric Ozone Budget: 1979â2010}},
