issn = {0944-1344},
journal = {Environmental Science and Pollution Research},
keywords = {Air pollution,Health impact assessment,Morbidity,Mortality,Oxidative stress,Respiratory diseases,Susceptible groups,air quality,pesquisa},
mendeley-tags = {air quality,pesquisa},
