	issn = {0016-2361},
	journal = {Fuel},
	keywords = {Aldehydes, Air pollution, Megacity, NO, Vehicular emissions},
	pages = {505-513},
	title = {Formaldehyde and acetaldehyde measurements in urban atmosphere impacted by the use of ethanol biofuel: Metropolitan Area of Sao Paulo (MASP), 2012--2013},
	url = {https://www.sciencedirect.com/science/article/pii/S0016236114005535},
	volume = {134},
	year = {2014},
	Bdsk-Url-1 = {https://www.sciencedirect.com/science/article/pii/S0016236114005535},
