author = {Dominutti, Pamela A. and Nogueira, Thiago and Borbon, Agn{\`{e}}s and Andrade, Maria de Fatima and Fornaro, Adalgiza},
doi = {10.1016/j.atmosenv.2016.08.008},
file = {:Users/adelgado/Library/Application Support/Mendeley Desktop/Downloaded/Dominutti et al. - 2016 - One-year of NMHCs hourly observations in S{\~{a}}o Paulo megacity meteorological and traffic emissions effects in a.pdf:pdf},
issn = {13522310},
journal = {Atmospheric Environment},
keywords = {Air pollution,Automatic sampling,Biofuel,Megacity,Non-methane hydrocarbons,Vehicular emissions,air quality},
mendeley-tags = {air quality},
month = {oct},
pages = {371--382v},
title = {{One-year of NMHCs hourly observations in S{\~{a}}o Paulo megacity: meteorological and traffic emissions effects in a large ethanol burning context}},
